{
    "author": "neolao",
    "license": "MIT",
    "name": "sysdashboard",
    "description": "A dashboard for metrics",
    "version": "0.0.3",
    "repository": {
        "type": "git",
        "url": "https://github.com/neolao/sysdashboard.git"
    },
    "engines": {
        "node": ">=0.12"
    },
    "bin": {
        "sysdashboard-node": "./node-harmony.sh",
        "sysdashboard": "./bundle-es5/console.js"
    },
    "main": "./bundle-es5/Sysdashboard.js",
    "dependencies": {
        "solfegejs"         : "1.4.*",
        "solfegejs-cli"     : "1.3.*",
        "babel-runtime"     : "5.6.15"
    },
    "devDependencies": {
        "babel"         : "5.6.14"
    },
    "preferGlobal": true,
    "keywords": [
        "dashboard"
    ],
    "scripts"       :
    {
        "console"       : "node --harmony --harmony-proxies ./bundle-es5/console.js",
        "es6-watch"     : "node_modules/.bin/babel bundle-es6 --optional runtime --watch --out-dir bundle-es5"
    }

}
